@page "~/Categorias/{skip?}/{take?}"
@model MyApp.Namespace.CategoriesModel

@{
  ViewData["Title"] = "Categorias";
}

<table>
  <thead>
    <tr>
      <td>ID</td>
      <td>Titulo</td>
      <td>Preço</td>
    </tr>
  </thead>
  <tbody>
    @foreach (var category in Model.CategoryList)
    {
      <tr>
        <td>@category.Id</td>
        <td>@category.Title</td>
        <td>@category.Price</td>
      </tr>
    }
  </tbody>
</table>
